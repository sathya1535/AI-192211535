import networkx as nx

def dfs_path(graph, start, goal):
    return nx.dfs_edges(graph, source=start)

if __name__== "__main__":
    graph = nx.Graph({'A': ['B', 'C'], 'B': ['D', 'E'], 'C': ['F'], 'D': [], 'E': [], 'F': []})
    path = list(nx.dfs_edges(graph, source='A'))
    print("Path toÂ goal:",path)
